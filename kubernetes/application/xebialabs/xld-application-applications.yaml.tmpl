apiVersion: xl-deploy/v1
kind: Applications
spec:
- name: {{.AppName}}
  type: udm.Application
  children:
  - name: {{.DockerImageTag}}
    type: udm.DeploymentPackage
    deployables:
    - name: {{.AppName}}
      type: k8s.ResourcesFile
      file: !file "../kubernetes-application/deployment.yaml"
      tags:
      - {{.NamespaceTag}}
