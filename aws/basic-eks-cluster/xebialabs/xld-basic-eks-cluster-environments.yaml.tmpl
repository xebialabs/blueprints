{{$app := .ClusterName | kebabcase}}

apiVersion: xl-deploy/v1
kind: Environments
spec:
- name: {{$app}}
  type: core.Directory
  children:
  - name: aws-cloudformation-{{$app}}
    type: udm.Environment
    members:
    - Infrastructure/{{$app}}/aws-{{$app}}
    - Infrastructure/{{$app}}/aws-{{$app}}/eks-cloudformation
